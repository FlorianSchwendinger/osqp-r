set -x
set -e

tail DESCRIPTION

pwd
cd osqp_sources
pwd

tail CMakeLists.txt

which cmake
cmake -DUNITTESTS=OFF .
cmake --build .

cd ..


# Linux
cp ./osqp_sources/out/libosqpstatic.a src/ || echo "Failed: libosqpstatic.a -> src"

# Mac
cp ./osqp_sources/out/libosqpstatic.a src/ || echo "Failed: libosqpstatic.a -> src"
